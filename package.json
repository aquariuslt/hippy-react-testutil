    "eslint-config-prettier": "8.3.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "rollup": "^2.52.8",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
